<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane prefHeight="720.0" prefWidth="830.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.iut.pt.veto.form.HR.ListEmployeesController">
  <center>
    <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
      <children>
        <TableView fx:id="tabEmployee" layoutX="0.0" layoutY="120.0" prefHeight="600.0" prefWidth="830.0">
          <columns>
            <TableColumn fx:id="roleColumn" maxWidth="130.0" minWidth="130.0" prefWidth="130.0" text="Role" />
            <TableColumn fx:id="lastNameColumn" maxWidth="300.0" minWidth="100.0" prefWidth="200.0" text="Nom" />
            <TableColumn fx:id="firstNameColumn" maxWidth="300.0" minWidth="100.0" prefWidth="200.0" text="Prénom" />
            <TableColumn fx:id="detailColumn" maxWidth="100.0" minWidth="50.0" prefWidth="100.0" text="Détails" />
            <TableColumn fx:id="leaveColumn" maxWidth="150.0" minWidth="50.0" prefWidth="100.0" text="Congé" />
            <TableColumn fx:id="contractColumn" maxWidth="150.0" minWidth="50.0" prefWidth="100.0" text="Contrat" />
          </columns>
        </TableView>
            <Label fx:id="info" alignment="CENTER" prefHeight="100.0" prefWidth="830.0" textFill="#ff0707" />
      </children>
    </Pane>
  </center>
</BorderPane>
